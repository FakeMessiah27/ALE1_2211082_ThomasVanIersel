<Window x:Class="ALE1_2211082_ThomasVanIersel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ALE1_2211082_ThomasVanIersel"
        mc:Ignorable="d"
        Title="ALE1_2211082_ThomasVanIersel" Height="350" Width="750.33">
    <Grid>
        <Button x:Name="btnExecute" Content="Execute&#xD;&#xA;" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="btnExecute_Click"/>
        <TextBox x:Name="tbPrefix" HorizontalAlignment="Left" Height="23" Margin="10,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="379" Text="=( &gt;(A,B), |( ~(A) , =( B, &gt;( C, D ) )) )"/>
        <TextBox x:Name="tbInfix" HorizontalAlignment="Left" Height="23" Margin="71,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="1.557,0.673"/>
        <TextBox x:Name="tbVariables" HorizontalAlignment="Left" Height="23" Margin="71,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

        <Label Content="Infix:" HorizontalAlignment="Left" Margin="10,82,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.092,-3.829"/>
        <Label Content="Variables:" HorizontalAlignment="Left" Margin="10,107,0,0" VerticalAlignment="Top"/>
        <Label Content="Formula (as prefix):" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top"/>

        <Image x:Name="graph" Height="269" Margin="0,40,30,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="299"/>
        <Label Content="Graph:" HorizontalAlignment="Left" Margin="540,14,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
